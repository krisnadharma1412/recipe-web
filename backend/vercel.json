{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "2048mb", 
                "prisma": {
                    "generate": "node_modules/.bin/prisma generate"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        }
    ]
}